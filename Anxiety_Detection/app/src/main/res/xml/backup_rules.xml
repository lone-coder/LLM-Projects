<?xml version="1.0" encoding="utf-8"?>
<!--
   Sample backup rules file; uncomment and customize as necessary.
   See https://developer.android.com/guide/topics/data/autobackup
   for details.
   Note: This file is ignored for devices older than API 31
   See https://developer.android.com/about/versions/12/backup-restore
-->
<full-backup-content>
    <!-- Include all shared preferences... -->
    <include domain="sharedpref" path="."/>
    <!-- ...but exclude a specific shared preference file. -->
    <exclude domain="sharedpref" path="device.xml"/>

    <!-- Include the entire database domain... -->
    <include domain="database" path="."/>
    <!-- ...and then exclude the specific anxiety_database file. -->
    <exclude domain="database" path="anxiety_database"/>
</full-backup-content>
